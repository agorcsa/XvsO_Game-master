<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">

            <corners
                android:bottomLeftRadius="30dp"
                android:bottomRightRadius="30dp"
                android:radius="60dp"
                android:topLeftRadius="30dp"
                android:topRightRadius="30dp" />

            <solid android:color="#efae66" />

            <padding
                android:bottom="0dp"
                android:left="0dp"
                android:right="0dp"
                android:top="0dp" />

            <size
                android:height="30dp"
                android:width="200dp" />
        </shape>
    </item>
    <item android:drawable="@drawable/ic_baseline_refresh_24" />
</layer-list>
